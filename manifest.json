{
    "manifest_version": 3,
    "name": "TabTagger",
    "description": "Enter a new title in the input field for any open tab to update it instantly, click on the tab icon or URL to switch to it.",
    "version": "0.0.0.1",
    "permissions": [
        "tabs",
        "scripting"
    ],
    "icons": {
        "16": "images/TabTaggerLogo16.png",
        "32": "images/TabTaggerLogo32.png",
        "48": "images/TabTaggerLogo48.png",
        "128": "images/TabTaggerLogo128.png"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    }
}